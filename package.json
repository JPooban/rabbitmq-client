{
  "name": "rabbitmq-client-node",
  "version": "1.0.5",
  "description": "A simple rabbitmq client for nodejs",
  "main": "index.js",
  "files": [
    "/lib"
  ],
  "scripts": {
    "start:pub": "DEBUG=rabbitmq-client,example-publisher node ./examples/publisher.js",
    "start:sub": "DEBUG=rabbitmq-client,example-subscriber node ./examples/subscriber.js",
    "lint": "eslint --ext .js ./",
    "test": "jest --detectOpenHandles --forceExit"
  },
  "keywords": [
    "amqp",
    "rabbitmq",
    "rabbitmq client",
    "amqplib",
    "messaging queue",
    "amqp client",
    "message broker",
    "nodejs"
  ],
  "author": {
    "name": "Jugkapong Pooban",
    "email": "j.pooban@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "amqplib": "^0.5.6",
    "debug": "^4.1.1",
    "lodash.once": "^4.1.1"
  },
  "devDependencies": {
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "sinon": "^9.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
